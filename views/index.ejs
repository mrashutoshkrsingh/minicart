        <button onclick="window.location.href='/products'" class="btn btn-primary" id="s1">Shop</button>
      </div>
      </div>

      <div class="item">
        <img src="https://mediaindia.eu/wp-content/uploads/2017/07/Ecommerce-BTL-768x543.jpg" alt="Chicago" style="width:100%;">
      </div>
    
      <div class="item">
        <img 
        src="https://cdn.vox-cdn.com/thumbor/8TLYMgXwTAOX299oBX6fZiWNs78=/71x0:1208x853/920x613/filters:focal(71x0:1208x853):format(webp)/cdn.vox-cdn.com/uploads/chorus_image/image/50030233/iphones-1685.0.jpg" alt="New york" style="width:100%;">
      </div>
    </div>

    <!-- Left and right controls -->
    <a class="left carousel-control" href="#myCarousel" data-slide="prev">
      <span class="glyphicon glyphicon-chevron-left"></span>
      <span class="sr-only">Previous</span>
    </a>
    <a class="right carousel-control" href="#myCarousel" data-slide="next">
