      <li data-target="#myCarousel" data-slide-to="1"></li>
      <li data-target="#myCarousel" data-slide-to="2"></li>
    </ol>

    <!-- Wrapper for slides -->
    <div class="carousel-inner">
      <div class="item active">
        <img src="https://lh3.googleusercontent.com/s5AnbPuA7noB3004GhmVZ3N-T6M8vqoI2aJszikJXknY2pOKC5F1RFEEeHe__I9ITBkK7xc5-VySz-Q577b-90o8Y6A-vFWE=s750" alt="Los Angeles" style="width:100%;">
